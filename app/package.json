{
  "name": "clay-starter",
  "version": "0.0.1",
  "description": "The started for Clay",
  "main": "app.js",
  "scripts": {
    "test": "mocha \"./{,!(node_modules)/**/}*.test.js\"",
    "start": "npm run check-elastic && nodemon --delay 2 --exec \"node -r dotenv/config\" app.js",
    "check-elastic": "while ! curl -sSI -o /dev/null http://elasticsearch:9200/_cat/indices; do sleep 5; done",
    "build": "CLAYCLI_COMPILE_MINIFIED_TEMPLATES=true LOG=info ./node_modules/.bin/clay compile --globs 'global/js/**/!(*.test).js' --inlined --linked --reporter pretty && LOG=info ./node_modules/.bin/clay compile custom-tasks",
    "build-production": "CLAYCLI_COMPILE_MINIFIED_TEMPLATES=true LOG=info ./node_modules/.bin/clay compile --minify --globs 'global/js/**/!(*.test).js' --reporter pretty && LOG=info ./node_modules/.bin/clay compile custom-tasks",
    "watch": "CLAYCLI_COMPILE_MINIFIED_TEMPLATES=true LOG=info ./node_modules/.bin/clay compile --globs 'global/js/**/!(*.test).js' --inlined --linked --watch",
    "build-web-player": "npm install git+ssh://git@github.com/Entercom/radio-web-player.git#develop --no-save && mkdir -p public/web-player && cd ./node_modules/radio-web-player && npm install && npm run build && cd demo-site && npm install && npm run build && cp -r dist/* ../../../public/web-player/",
    "eslint": "npx eslint . --cache"
  },
