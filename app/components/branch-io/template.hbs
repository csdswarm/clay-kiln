<!-- data-uri="{{ default _ref _self }}" -->
{{#unless @root.locals.edit}}
    <script src="https://cdn.branch.io/branch-2.49.0.min.js"></script>
    <script type="text/javascript">
        // init Branch
        branch.init("{{ _computed.branchIoKey }}");
    </script>
    {{#each _computed.player_site_genre }}
        <meta name="branch:deeplink:player_site_genre" content="{{ name }}" />
    {{/each}}
    <meta name="branch:deeplink:station_logo" content="{{ _computed.station_logo }}" />
    <meta name="branch:deeplink:player_site_category" content="{{ _computed.player_site_category }}" />
    <meta name="branch:deeplink:player_site_market" content="{{ _computed.player_site_market }}" />
    <meta name="branch:deeplink:page" content="{{ _computed.page }}" />
    <meta name="branch:deeplink:station_name" content="{{ _computed.station_name }}" />
    <meta name="branch:deeplink:station_id" content="{{ _computed.station_id }}" />
{{/unless}}