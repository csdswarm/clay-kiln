<!DOCTYPE html>
<html lang="en" data-uri="{{ default _ref _self }}" data-layout-uri="{{ default _ref _layoutRef }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    {{#unless (includes (default _ref _self) "@published") }}
      <meta name="robots" content="noindex">
    {{/unless}}

    {{! head components }}
    <!-- data-editable="head" -->
      {{> component-list head }}
    <!-- data-editable-end -->
    <!-- data-editable="headLayout" -->
      {{> component-list headLayout }}
    <!-- data-editable-end -->
  </head>
  <body class="two-column-layout">
    <section class="top" data-editable="top">{{> component-list top }}</section>
    <header class="page-header" data-editable="pageHeader">{{> component-list pageHeader }}</header>
    <section class="section-header" data-editable="sectionHeader">{{> component-list sectionHeader }}</section>
    <section class="wrapper">
      <section class="main" data-editable="main">{{> component-list main }}</section>
      <section class="tertiary" data-editable="tertiary" data-track-zone="tertiary">{{> component-list tertiary }}</section>
      <section class="secondary" data-editable="secondary" data-track-zone="secondary">{{> component-list secondary }}</section>
    </section>
    <footer class="bottom" data-editable="bottom">{{> component-list bottom }}</footer>
    <div class="kiln-internals" data-editable="kilnInternals">{{> component-list kilnInternals }}</div>
  </body>
</html>
