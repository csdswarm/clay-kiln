<div
        class="component component--podcast-set podcast-set"
        data-uri="{{ default _ref _self }}"
        data-set="{{setType}}"
>
  {{#if title}}
    <h2>{{title}}</h2>
  {{/if}}
  <ul class="podcast-set-list">
    {{#each _computed.podcasts as |podcast|}}
      <li class="podcast-set-item">
        <div class="podcast-set-show">
          <img src="{{podcast.attributes.image}}" alt="{{podcast.show.name}}" />
          <div class="show__name">{{podcast.show.name}}</div>
          <div class="show__details">{{podcast.attributes.description}}</div>
        </div>
      </li>
    {{else}}
      <li class="podcast-set-item podcast-set-tbd">
        TBD...
      </li>
    {{/each}}
  </ul>
</div>





